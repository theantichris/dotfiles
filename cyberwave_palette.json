{
  "name": "Cyberwave Universal Palette",
  "version": "1.0.0",
  "description": "Universal color palette extracted from the original Warp Cyberwave VSCode theme",
  "source": "https://raw.githubusercontent.com/8mitsuboy/warp-cyber-wave/refs/heads/main/themes/Warp%20Theme-color-theme.json",
  "palette": {
    "base": {
      "background": "#091a1f",
      "surface-1": "#192a30",
      "surface-2": "#001a24",
      "surface-3": "#08252f",
      "text": "#cdcefb",
      "text-muted": "#bdc2c3",
      "comment": "#8e9395"
    },
    "accents": {
      "primary": "#16e6c9",
      "secondary": "#26add0",
      "tertiary": "#c73df5e3",
      "quaternary": "#ff8ffd"
    },
    "semantics": {
      "success": "#23d18b",
      "warning": "#cca700",
      "error": "#e25e44",
      "info": "#75beff",
      "link": "#119c82"
    },
    "syntax": {
      "keyword": "#cdcefb",
      "string": "#26add0",
      "number": "#26add0",
      "function": "#c73df5e3",
      "variable": "#16e6c9",
      "type": "#b3f971",
      "constant": "#26add0",
      "operator": "#ff8ffd",
      "punctuation": "#cdcefb",
      "tag": "#16e6c9",
      "attribute": "#26add0"
    },
    "ui": {
      "border": "#029fec91",
      "shadow": "#5b3199bf",
      "selection": "#264f78",
      "highlight": "#ff349a",
      "focus": "#194341"
    },
    "terminal": {
      "black": "#000000",
      "red": "#cd3131",
      "green": "#0dbc79",
      "yellow": "#e5e510",
      "blue": "#2472c8",
      "magenta": "#bc3fbc",
      "cyan": "#11a8cd",
      "white": "#e5e5e5",
      "bright_black": "#666666",
      "bright_red": "#f14c4c",
      "bright_green": "#23d18b",
      "bright_yellow": "#f5f543",
      "bright_blue": "#3b8eea",
      "bright_magenta": "#d670d6",
      "bright_cyan": "#29b8db",
      "bright_white": "#e5e5e5"
    }
  },
  "usage_guide": {
    "backgrounds": {
      "primary": "base.background",
      "elevated": "base.surface-1",
      "panels": "base.surface-2",
      "sidebars": "base.surface-3"
    },
    "text_hierarchy": {
      "primary": "base.text",
      "secondary": "base.text-muted",
      "disabled": "base.comment"
    },
    "interactive_elements": {
      "buttons": "accents.primary",
      "links": "semantics.link",
      "selections": "ui.selection",
      "focus_rings": "accents.primary"
    },
    "code_highlighting": {
      "keywords": "syntax.keyword",
      "strings": "syntax.string",
      "functions": "syntax.function",
      "variables": "syntax.variable",
      "types": "syntax.type"
    }
  }
}